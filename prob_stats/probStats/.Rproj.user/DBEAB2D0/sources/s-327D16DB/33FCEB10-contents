ourData <- read.csv("myData.csv", TRUE, ",")
tauxCroissance <- ourData$Taux_Croissance_Pop
consommationAlcool <- ourData$Consommation_Alcool_Par_Habitant
alphabetisation <- ourData$Taux_Alphabetisation

