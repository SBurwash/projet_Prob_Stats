#Q1 -> correlation taux alcool - aphabetisation
#Statistique descriptives (tableaux, histogramme, etc.)
plot(ourData$Taux_Croissance_Pop, ourData$Consommation_Alcool_Par_Habitant)
boxplot(ourData$Consommation_Alcool_Par_Habitant, col="grey", horizontal = T)
boxplot(ourData$Taux_Croissance_Pop, col="grey", horizontal = T)

shapiro.test(tauxCroissance)

qqnorm(tauxCroissance, main ="Diagramme de probabilités normal - ou Q-Q Plot of x")
qqline(tauxCroissance)      # pour ajouter une droite

qqnorm(consommationAlcool, main ="Diagramme de probabilités normal - ou Q-Q Plot of x")
qqline(consommationAlcool)      # pour ajouter une droite

t.test(tauxCroissance, mu = 2, alternative = "greater")
hist(tauxCroissance)
tbl1 <- table(tauxCroissance, consommationAlcool)
chisq.test(tauxCroissance)
chisq.test(tauxCroissance)
